<div class="col-6 d-none all-forms" id="about">
    <form action="/admin/about" method="post" enctype="multipart/form-data" id="aboutForm">
        <h4>about wood</h4>
        <div class="mb-3">
            <label for="txt">content</label>
            <div>
                <textarea id="aboutinaEn">
                    <%if(about){%><%=about.en%><%}%>
                </textarea>
            </div>
            <input type="hidden" name="en" id="aboutEn">
        </div>
        <div class="mb-3">
            <label for="txt">المحتوى</label>
            <div>
                <textarea id="aboutinaAr">
                    <%if(about){%><%=about.ar%><%}%>
                </textarea>
            </div>
            <input type="hidden" name="ar" id="aboutAr">
        </div>
        <div class="mb-3" style="width: 50%;">
            <label for="">عدد العملاء</label>
            <input type="number" class="form-control" name="clints" value="<%if(about){%><%=about.clints%><%}%>">
        </div>
        <div class="mb-3" style="width: 50%;">
            <label for="">عدد المشاريع</label>
            <input type="number" class="form-control" name="projects" value="<%if(about){%><%=about.projects%><%}%>">
        </div>
        <div class="mb-3">
            <label for="txt">الصورة</label>
            <input type="file" name="img" class="form-control">
        </div>
        <button type="button" class="btn btn-dark" onclick="submitabout()">submit</button>
    </form>
</div>