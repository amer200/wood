<%- include('includes/head') %>
    <%- include('includes/loading-page') %>
        <%- include('includes/header',{page: "خدماتنا" }) %>

            <!-- Service Start -->
            <div class="container-xxl py-5">
                <div class="container">
                    <div class="section-title text-center">
                        <h1 class="display-5 mb-5">خدماتنا</h1>
                    </div>
                    <div class="row g-4">
                        <%let i=0 %>
                            <%servs.forEach(s=>{ %>
                                <div class="col-md-6 col-lg-4 wow fadeInUp" data-wow-delay="0.<%i++%>s">
                                    <div class="service-item">
                                        <div class="overflow-hidden">
                                            <img class="img-fluid" src="<%= s.img%>" alt="">
                                        </div>
                                        <div class="p-4 text-center border border-5 border-light border-top-0">
                                            <h4 class="mb-3">
                                                <%=s.title.ar%>
                                            </h4>
                                            <p>
                                                <%=s.content.ar%>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                                <%})%>
                    </div>
                </div>
            </div>

            <!-- Service End -->
            <%- include('includes/testo') %>
                <%- include('includes/footer') %>